            "sessionStampRotateCount": {
                "enabled": true,
                "description": "Total number of stamp rotation events over the session"
            },
            "sessionSingleRotateCount": {
                "enabled": true,
                "description": "Total number of single rotation events over the session"
            },
            "sessionMoleculeMoveCount": {
                "enabled": true,
                "description": "Total number of molecule move events over the session"
            },
            "sessionClearBtnPresses": {
                "enabled": true,
                "description": "Total number of times the clear button was pressed during the session"
            },
            "sessionMuseumDurationInSecs": {
                "enabled": true,
                "description": "Total amount of time spent in a museum during the session?"
            }
