            },
            "singleRotateCount": {
                "enabled": true,
                "description": "Number of single rotation events in the level"
            },
            "moleculeMoveCount": {
                "enabled": true,
                "description": "Number of molecule move (MOLECULE_RELEASE) events in the level"
            },
            "clearBtnPresses": {
                "enabled": true,
                "description": "Number of times the clear button was pressed in the level"
            },
            "avgMoleculeDragDurationInSecs": {
                "enabled": true,
                "description": "The average number of seconds each drag took, over the given level"
            },
            "finalScore": {
                "enabled": true,
                "description": "The max final score given when submit button pressed, for this level, among all plays"
            },
            "beginCount": {
                "enabled": true,
                "description": "number of times a player 'began' the level. [count of 'BEGIN' events]"
            },
            "menuBtnCount": {
                "enabled": true,
                "description": "The number of times a player pressed the 'return to menu' button during a level"
            },
            "completesCount": {
                "enabled": true,
                "description": "The number of time the player (pressed the submit button)/(completed the level) in the level"
            }
